{
  "name": "@domonda/plumb",
  "version": "2.2.6",
  "license": "ISC",
  "description": "Simple state management inspired by Observables.",
  "homepage": "https://github.com/domonda/domonda-js#readme",
  "keywords": [
    "domonda",
    "state",
    "observables",
    "plumb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domonda/domonda-js.git"
