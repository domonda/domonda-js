{
  "name": "@domonda/ui",
  "version": "1.0.2",
  "description": "UI framework developed and used by domonda.",
  "keywords": [
    "domonda",
    "ui",
    "framework",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/domonda/domonda-js#readme",
  "license": "ISC",
  "main": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domonda/domonda-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "clean": "rimraf ./lib/*",
    "clean:storybook": "rimraf ./build/*",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "build": "npm run clean && tsc && npm run build:copy-files",
    "build:storybook": "npm run clean:storybook && build-storybook -o ./build",
    "start": "tsc --watch --preserveWatchOutput",
    "start:storybook": "start-storybook --ci -p 6006"
  },
  "bugs": {
    "url": "https://github.com/domonda/domonda-js/issues"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "deepmerge": "^3.3.0",
    "downshift": "^3.2.10",
    "jss-plugin-camel-case": "^10.0.0-alpha.21",
    "jss-plugin-global": "^10.0.0-alpha.21",
    "jss-plugin-nested": "^10.0.0-alpha.21",
    "jss-plugin-props-sort": "^10.0.0-alpha.21",
    "jss-plugin-rule-value-function": "^10.0.0-alpha.21",
    "jss-plugin-vendor-prefixer": "^10.0.0-alpha.21",
    "popper.js": "^1.15.0",
    "react-jss": "^10.0.0-alpha.21",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react": "^8.0.4",
    "@testing-library/react-hooks": "^1.1.0",
    "@types/faker": "^4.1.5",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-virtualized": "^9.21.2",
    "@types/storybook__react": "^4.0.2",
    "babel-loader": "^8.0.6",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2"
  }
