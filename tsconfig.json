{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "node",
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "strict": true,
    "jsx": "react",
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "declaration": true
  },
  "exclude": ["node_modules"]
}
